{
  "name": "hhplus-server-concert",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start --watch",
    "debug": "nest start --debug --watch",
    "prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:setup": "make start-local && yarn migration:run:test",
    "test:local:setup": "docker compose --env-file .env.test.local -f docker/test-local/docker-compose.yml up -d && yarn migration:run:test-local",
    "test:local:integration": "yarn test:setup && jest --config ./test/jest-integration.json",
    "migration:run:test": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run -d typeorm/test-data-source.ts",
    "migration:run:test-local": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run -d typeorm/test-local-data-source.ts",
    "seed:run": "ts-node -r tsconfig-paths/register --project tsconfig.json ./node_modules/typeorm-extension/bin/cli.cjs seed:run -d typeorm/data-source.ts",
    "seed:run:test": "ts-node -r tsconfig-paths/register --project tsconfig.json ./node_modules/typeorm-extension/bin/cli.cjs seed:run -d typeorm/test-data-source.ts",
    "seed:create": "ts-node ./node_modules/typeorm-extension/bin/cli.cjs seed:create",
    "prepare": "ts-patch install && typia patch"
  },
  "dependencies": {
    "@js-joda/core": "^5.6.3",
    "@js-joda/timezone": "^2.21.1",
    "@nestia/core": "^3.4.3",
    "@nestia/e2e": "^0.6.0",
    "@nestia/fetcher": "^3.4.3",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.0.0",
    "@nestjs/cqrs": "^10.2.7",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/schedule": "^4.1.0",
    "@nestjs/swagger": "^7.4.0",
    "@nestjs/typeorm": "^10.0.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "decimal.js": "^10.4.3",
    "ioredis": "^5.4.1",
    "mysql2": "^3.10.2",
    "nest-winston": "^1.9.7",
    "redlock": "^5.0.0-beta.2",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20",
    "typeorm-transactional": "^0.5.0",
    "typia": "^6.4.1",
    "ulid": "^2.3.0",
    "winston": "^3.13.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@nestia/sdk": "^3.4.3",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "jest-mock-extended": "^3.0.7",
    "nestia": "^5.3.1",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.2",
    "ts-patch": "^3.2.1",
    "tsconfig-paths": "^4.2.0",
    "typeorm-extension": "^3.5.1",
    "typescript": "5.5.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@lib$": "<rootDir>/lib",
      "^@lib/(.*)$": "<rootDir>/lib/$1",
      "^@config$": "<rootDir>/config",
      "^@config/(.*)$": "<rootDir>/config/$1"
    }
  }
}
